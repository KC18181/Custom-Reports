<?xml version="1.0" encoding='UTF-8'?>
<odoo>
<data noupdate="1">
    <record id="brand_scheduler" model="ir.cron">
       <field name="name">Brand Field</field>
       <field name="model_id" ref="model_sales_brand"/>
       <field name="state">code</field>
       <field name="code">model.brand()</field>
       <field name="user_id" ref="base.user_root"/>
       <field name='interval_type'>days</field>
       <field name="numbercall">-1</field>
       <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 12:00:00')" />
   </record>
</data>
</odoo>
